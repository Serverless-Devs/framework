{
  "private": true,
  "scripts": {
    "publish": "lerna publish",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "f2elint scan -i src",
    "fix": "f2elint fix",
    "typecheck": "npx tsc -p tsconfig.json --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^12.12.6",
    "f2elint": "^0.4.4",
    "git-cz": "^4.7.6",
    "jest": "^26.4.0",
    "lerna": "^3.22.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "commit-msg": "f2elint exec commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "f2elint exec lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts}": "f2elint exec eslint"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "dependencies": {
    "fs-extra": "^9.1.0",
    "lodash": "^4.17.21"
  }
}
